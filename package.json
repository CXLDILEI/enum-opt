{
  "name": "enum-opt",
  "version": "1.3.1",
  "description": "typescript enum to options",
  "main": "./lib/utils.js",
  "scripts": {
    "dev": "rollup -cw --bundleConfigAsCjs",
    "build": "rollup -c --bundleConfigAsCjs",
    "test": "vitest",
    "lint": "eslint --fix",
    "prepublish": "pnpm build",
    "release": "release-it",
    "test:run": "vitest run"
  },
  "keywords": [
    "typescript",
    "enum",
    "options",
    "transform"
  ],
  "repository": "git@github.com:CXLDILEI/enum-opt.git",
  "author": "TYF",
  "license": "MIT",
  "bin": "./lib/index.js",
  "files": [
    "README.md",
    "lib",
    "template",
    "package.json",
    "LICENSE",
    "types"
  ],
  "typings": "types/utils.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/types": "^7.20.7",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "release-it": "^15.6.0",
    "rollup": "^3.25.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "rollup-plugin-uglify": "^6.0.4",
    "vitest": "^0.28.5"
  },
  "dependencies": {
    "@babel/parser": "^7.20.13",
    "@babel/traverse": "^7.20.13",
    "core-js": "^3.33.3",
    "ejs": "^3.1.8",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "@babel/parser": "^7.20.13",
    "@babel/traverse": "^7.20.13",
    "ejs": "^3.1.8",
    "typescript": "^4.9.4"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "hooks": {
      "before:init": [
        "pnpm run lint",
        "pnpm test:run"
      ],
      "after:release": "echo Successfully released ${name} v${version} to ${repo.repository}."
    }
  },
  "exports": {
    ".": "./lib/utils.js",
    "./core": "./lib/core.js"
  }
}
